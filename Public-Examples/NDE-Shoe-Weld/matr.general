symb bepxx = 1305.8 * 1.041039			/* dielectric constant (constant strain)
symb bepzz = 1200.2 * 1.236313			/* dielectric constant (constant strain)
symb brho  = 7820. * 1.010288			/* density
symb bqdmp = 90.  	 				/* Mechanical Q at 1e6
c
