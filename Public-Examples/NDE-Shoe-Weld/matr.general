c -------------------------------------------------------------------------
c    fibr : fiberglass isotropic
c -------------------------------------------------------------------------
c    
     type elas
     symb prsh = 0.3
     symb ymsh = 21.e9
     symb sdens = 1880.
     symb sbulk = $ymsh / ( 3. * ( 1 - ( 2. * $prsh ) ) )
     symb ssher = $ymsh / ( 2. * ( 1 + $prsh ) )
     symb vl1 = ( ( $sbulk + ( 4. * $ssher / 3. ) ) / $sdens ) ** 0.5
     symb vs1 = ( $ssher / $sdens ) ** 0.5
     prop fibr  $sdens $vl1 $vs1 
