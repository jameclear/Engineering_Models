symb bc11 = 1.3745e11   				/* stiffness constant (constant electric field)
symb bc33 = 1.2574e11 * 1.081938		/* stiffness constant
symb bc44 = 2.2279e10 * 0.5313475		/* stiffness constant
symb bc12 = 0.8790e11 * 0.9634085		/* stiffness constant
symb bc13 = 0.9230e11 * 1.0852169		/* stiffness constant
c symb bc66 = .5 * ( $bc11 - $bc12 )  
symb bc66 = 2.4779e10 * 0.6995162
